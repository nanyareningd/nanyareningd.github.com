        document.write('<img class="img-responsive" src="http://nanyareningd.gitcafe.io/images/post/20151205/majiang1.png">');
    }
    else{
        document.write('<img class="img-responsive" src="http://nanyareningd.gitcafe.io/images/mobile/post/20151205/zhuoqiu.jpg">');
